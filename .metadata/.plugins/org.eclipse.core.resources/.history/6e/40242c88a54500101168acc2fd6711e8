<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="True" scanPeriod="30 seconds">
	<appender name="ConsoleAppender"
		class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<Pattern>%d{[yyyy.MM.dd HH:mm:ss.SSS]} [%-5level] [%thread] %msg
				\(%F:%L\) %n</Pattern>
		</encoder>
	</appender>
</configuration>